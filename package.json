  "private": true,
  "scripts": {
    "start": "npm run production",
    "dev": "npm-run-all development:client:rendering-service:build:prepare development",
    "development": "npm-run-all --parallel development:client:build development:client:rendering-service:build development:services",
    "development:client:rendering-service:build:prepare": "npx cross-env UNIVERSAL_WEBPACK_CSS_LOADER_V4=true NODE_PATH=src NODE_ENV=development APIPORT=3737 PORT=3000 universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "development:client:build": "npx cross-env UNIVERSAL_WEBPACK_CSS_LOADER_V4=true HOST=127.0.0.1 NODE_PATH=src NODE_ENV=development APIPORT=3737 PORT=3000 webpack serve --progress --config ./webpack/webpack.config.client.development.babel.js",
    "development:client:rendering-service:build": "npx cross-env UNIVERSAL_WEBPACK_CSS_LOADER_V4=true HOST=127.0.0.1 NODE_PATH=src NODE_ENV=development APIPORT=3737 PORT=3000 webpack --mode development --config ./webpack/webpack.config.server.development.babel.js --watch --color",
    "development:api": "npx cross-env UNIVERSAL_WEBPACK_CSS_LOADER_V4=true HOST=127.0.0.1 NODE_PATH=api NODE_ENV=development APIPORT=3737 PORT=3000 nodemon --trace-warnings ./bin/api.js --watch ./api --no-update-notifier",
    "development:client:rendering-service": "npx cross-env UNIVERSAL_WEBPACK_CSS_LOADER_V4=true NODE_PATH=src NODE_ENV=development APIPORT=3737 PORT=3000 nodemon --trace-warnings ./rendering-service/index.entry.js --watch ./rendering-service --watch ./build/server --no-update-notifier",
    "development:services": "npm-run-all --parallel development:client:rendering-service",
    "production": "npm-run-all production:client:build production:client:rendering-service:build production:services",
    "production:client:build": "webpack --mode production --color --config ./webpack/webpack.config.client.production.babel.js",
    "production:client:rendering-service:build": "webpack --mode production --color --config ./webpack/webpack.config.server.production.babel.js",
    "production:api": "cross-env NODE_ENV=production node ./api/index.entry.js",
    "production:client:rendering-service": "cross-env NODE_ENV=production node ./rendering-service/index.entry.js",
    "production:proxy": "cross-env NODE_ENV=production node ./proxy-server/index.entry.js",
    "production:services": "npm-run-all --parallel production:api production:client:rendering-service production:proxy",
    "build-old": "npx cross-env NODE_ENV=production webpack-cli --color --config=webpack/prod.config.js",
    "build-profile": "cross-env NODE_ENV=production webpack --color --config=webpack/prod.config.js --profile --json > stats.json",
    "dev-old": "npx concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "docker-build": "docker-compose build",
    "docker-dev": "npx cross-env NGINX_HOST=127.0.0.1 NGINX_PORT=3000 PORT=3000 docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --no-deps",
    "docker-down": "docker-compose down",
    "docker-up": "npx cross-env NGINX_HOST=127.0.0.1 NGINX_PORT=80 PORT=9777 docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --no-deps -d",
    "tests": "npx cross-env NODE_PATH=api NODE_ENV=development APIPORT=3737 node ./bin/tests.js",
    "geonames-download-zips": "npx cross-env NODE_PATH=api NODE_ENV=development APIPORT=3737 node ./bin/geonames-download-zips.js",
    "get-postal-codes-latlng": "npx cross-env NODE_PATH=api NODE_ENV=development APIPORT=3737 node ./bin/get-postal-codes-latlng.js",
    "get-postal-codes-latlng-prod": "npx cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/get-postal-codes-latlng.js",
    "insert-datas-dev": "npx cross-env NODE_PATH=api NODE_ENV=development APIPORT=3737 node ./bin/insert-datas.js",
    "insert-datas-prod": "npx cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/insert-datas.js",
    "install-db-dev": "npx cross-env NODE_PATH=api NODE_ENV=development APIPORT=3737 node ./bin/install-db.js",
    "install-db-prod": "npx cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/install-db.js",
    "import-db-dev": "npx cross-env NODE_PATH=api NODE_ENV=development APIPORT=3737 node ./bin/import-db.js",
    "import-db-prod": "npx cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/import-db.js",
    "lighthouse-report": "lighthouse http://127.0.0.1:9777 --view",
    "lint": "eslint --fix -c .eslintrc api src",
    "precommit": "lint-staged",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc './+(src|api)/**/*.js'",
    "start-old": "npx concurrently --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-dev": "npx cross-env HOST=127.0.0.1 TZ=UTC NODE_PATH=src NODE_ENV=development PORT=3000 APIPORT=3737 node --trace-deprecation ./bin/server.js",
    "start-dev-api": "npx cross-env HOST=127.0.0.1 TZ=UTC NODE_PATH=api NODE_ENV=development APIPORT=3737 node --trace-deprecation ./bin/api.js",
    "start-prod": "npx cross-env HOST=127.0.0.1 NODE_PATH=src NODE_ENV=production PORT=9777 APIPORT=5757 node --trace-deprecation ./bin/server.js",
    "start-prod-api": "npx cross-env HOST=127.0.0.1 NODE_PATH=api NODE_ENV=production APIPORT=5757 node --trace-deprecation ./bin/api.js",
    "test": "npx cross-env NODE_PATH=src jest src",
    "test-node": "npx cross-env NODE_PATH=api jest api",
    "utils": "npx cross-env NODE_PATH=api NODE_ENV=production APIPORT=3737 node ./bin/utils.js",
    "utils-dev": "npx cross-env NODE_PATH=api NODE_ENV=development APIPORT=3000 node ./bin/utils.js",
    "watch-client": "npx cross-env UV_THREADPOOL_SIZE=1000 NODE_PATH=src NODE_ENV=development PORT=3000 APIPORT=3737 node --trace-deprecation webpack/webpack-dev-server.js",
    "update-packages": "npm i -g npm-check-updates && ncu -u && npm install",
    "update-react-imports": "npx react-codemod update-react-imports --force"
  },
  "lint-staged": {
    "+(src|api)/**/*.js": [
      "eslint -c .eslintrc",
      "git add"
    ]
  },
